<?xml version="1.0" encoding="UTF-8"?>

<template namespace="DribbbleWebBoard_iPhone">

	<linear orientation="v" class="wrapper">
		<web id="web"/>
		<DribbbleWebBoardTab_iPhone id="tabbar"/>
	</linear>

	<style type="text/css">
		
		.wrapper {
			width: 100%;
			height: 100%;
		}
		
		#web {
			position: absolute;
			left: 0px;
			top: 0px;
			width: 100%;
			height: 100%;
			padding-bottom: 44px;
		}
		
		#tabbar {
			position: absolute;
			left: 0px;
			bottom: 0px;
			width: 100%;
			height: 44px;
		}

	</style>

</template>
